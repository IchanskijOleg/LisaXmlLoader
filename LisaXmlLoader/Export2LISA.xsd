	<!-- параметры, перекрывают поведение, заданное константами системы-->
	<xsd:complexType name="ParametersType">
		<xsd:sequence>
			<xsd:element name="loaddocument" type="xsd:int" minOccurs="0"/>
			<xsd:element name="setaccountsdepartmentstatus" type="xsd:int" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- информация по риску -->
	<xsd:complexType name="InsuranceRiskDescriptionType">
		<xsd:sequence>
			<xsd:element name="RiskId" type="xsd:int"/>
			<xsd:element name="ForIndividualTypeID" type="ForIndividualType"/>
			<xsd:element name="ND" type="xsd:string" minOccurs="0"/>
			<xsd:element name="NDTERM" type="xsd:string" minOccurs="0"/>
			<xsd:element name="NR" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BR" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BRA" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AGEEXTRAPREMIUM" type="xsd:string" minOccurs="0"/>
			<xsd:element name="NETTOCORRECTION" type="xsd:string" minOccurs="0"/>
			<xsd:element name="RiskAmount" type="xsd:string" minOccurs="0"/>
			<xsd:element name="RiskAmount2" type="xsd:string" minOccurs="0"/>
			<xsd:element name="isWopProtected" type="xsd:string" minOccurs="0"/>
			<xsd:element name="WopPrimaryPayTerm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PayTerm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="WaitTerm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Term" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PerpetualPayments" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PerpetualAnnuity" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PerpetualTerm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnnuityFrequency" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnnuityTerm" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AnnuityTypeid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Reinsure" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ReinsurerAgreementid" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IndividualRid" type="xsd:string" minOccurs="0"/>
			<!-- RiskId: Код риска -->
			<!-- ForIndividualTypeID: К кому относится риск-->
			<!-- ND: надбавка ND на уровне риска указывается в промилле-->
			<!-- NDTERM: Термін дії нетто-надбавки ND-->
			<!-- NR: Нетто-надбавка у відсотках-->
			<!-- BR: Брутто-надбавка у відсотках-->
			<!-- BRA: Комісія агента-->
			<!-- AGEEXTRAPREMIUM: Надбавка до віку-->
			<!-- NETTOCORRECTION: Коеф. к нетто-тарифу в процентах-->
			<!-- RiskAmount: Страховая сумма в копейках-->
			<!-- RiskAmount2: Страховая сумма (2) в копейках-->
			<!-- isWopProtected: Защищен риском WOP (1: да, 0: нет)-->
			<!-- WopPrimaryPayTerm: Срок защиты WOP-->
			<!-- PayTerm: Срок уплаты взносов. Если не задан, будет использован PayTerm для группы-->
			<!-- WaitTerm: Срок ожидания риска-->
			<!-- Term: Срок действия риска-->
			<!-- PerpetualPayment: Взносы платятся пожизненно (1: да, 0: нет)-->
			<!-- PerpetualAnnuity: Пожизненный аннуитет (1: да, 0: нет)-->
			<!-- PerpetualTerm: Риск действует пожизненно (1: да, 0: нет)-->
			<!-- annuityfrequency: Частота выплаты аннуитета (M, Q, H, Y)-->
			<!-- annuityterm: Срок выплаты аннуитета-->
			<!-- annuitytypeid: Тип аннуитета-->
			<!-- reinsure: Перестраховывать (1: да, 0: нет)-->
			<!-- reinsureragreementid: Код договора с перестраховщиком-->
		</xsd:sequence>
	</xsd:complexType>
	<!-- перечисление рисков входящих в группу -->
	<xsd:complexType name="InsuranceRiskDescriptionsType">
		<xsd:sequence>
			<xsd:element name="InsuranceRiskDescription" type="InsuranceRiskDescriptionType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Описание группы рисков полиса -->
	<xsd:complexType name="InsuranceRiskType">
		<xsd:sequence>
			<xsd:element name="InsuranceRiskGroupId" type="xsd:int"/>
			<xsd:element name="RiskAmount" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PayAmount" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PayTermType" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="M"/>
						<xsd:enumeration value="Y"/>
					</xsd:restriction>
				</xsd:simpleType>
				<!-- Не используется. Оставлено для обратной совместимости -->
			</xsd:element>
			<xsd:element name="InsuranceRiskDescriptions" type="InsuranceRiskDescriptionsType" minOccurs="0"/>
			<xsd:element name="PayTerm" type="xsd:double"/>
			<!-- Группа рисков, код -->
			<!-- Страховая сумма в валюте страхования, в копейках -->
			<!-- Страховой взнос, в копейках -->
			<!-- Длительность страхового цикла. Определяет, в каких единицах задается PayTerm (в годах или месяцах)-->
			<!-- Описание рисков, входящих в группу -->
			<!-- Срок уплаты взносов для всей группы-->
		</xsd:sequence>
	</xsd:complexType>
	<!-- Описание типа секции cтрахуемых рисков -->
	<xsd:complexType name="InsuranceRisksType">
		<xsd:sequence>
			<xsd:element name="InsuranceRisk" type="InsuranceRiskType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
